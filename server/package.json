{
  "name": "creator-portfolio-server",
  "version": "1.0.0",
  "description": "Backend API server for the Creator Portfolio Platform - A comprehensive platform for creators to showcase their work and connect with others.",
  "main": "server.js",
  "scripts": {
    "dev": "nodemon server.js",
    "start": "node server.js",
    "build": "npm install",
    "test": "jest --detectOpenHandles --forceExit",
    "test:watch": "jest --watch --detectOpenHandles",
    "test:coverage": "jest --coverage --detectOpenHandles --forceExit",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "seed": "node seedAdmin.js && node seedTestUsers.js",
    "seed:portfolio": "node seedPortfolio.js",
    "seed:admin": "node seedAdmin.js",
    "seed:users": "node seedTestUsers.js"
  },
  "keywords": [
    "api",
    "backend",
    "express",
    "mongodb",
    "portfolio",
    "creators",
    "authentication",
    "file-upload"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": "20.x",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "cloudinary": "^1.41.3",
    "compression": "^1.8.1",
    "connect-redis": "^9.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.21.2",
    "express-rate-limit": "^8.1.0",
    "express-session": "^1.18.2",
    "express-validator": "^7.3.0",
    "helmet": "^7.2.0",
    "isomorphic-dompurify": "^2.30.1",
    "jsonwebtoken": "^9.0.2",
    "ml-knn": "^3.0.0",
    "mongoose": "^8.17.0",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "multer-storage-cloudinary": "^4.0.0",
    "nodemailer": "^6.10.1",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "redis": "^5.9.0",
    "validator": "^13.15.15",
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^9.1.3",
    "nodemon": "^3.1.10",
    "supertest": "^6.3.3"
  }
